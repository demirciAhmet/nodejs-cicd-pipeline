# Node.js
node_modules/          # Exclude dependencies installed by npm
npm-debug.log*         # Exclude npm debug logs
yarn-debug.log*        # Exclude Yarn debug logs
yarn-error.log*        # Exclude Yarn error logs
.env                   # Exclude environment variable files
.DS_Store              # macOS system file
*.log                  # Exclude log files
coverage/              # Exclude test coverage reports
dist/                  # Exclude build output (if applicable)

# Ansible
*.retry                # Exclude retry files generated by Ansible
inventory/host_vars/   # Exclude host-specific variables (if sensitive)
inventory/group_vars/  # Exclude group-specific variables (if sensitive)
secrets/               # Exclude any directory used for sensitive data
vault_pass.txt         # Exclude Ansible Vault password file
ansible.cfg            # Exclude Ansible configuration file (if customized and sensitive)

# Terraform (if used in the project)
*.tfstate              # Exclude Terraform state files
*.tfstate.backup       # Exclude Terraform state backup files
.terraform/            # Exclude Terraform working directory
terraform.tfvars       # Exclude Terraform variable files (if sensitive)

# General
*.swp                  # Exclude swap files created by text editors
*.bak                  # Exclude backup files
*.tmp                  # Exclude temporary files
.idea/                 # Exclude IDE configuration files (e.g., JetBrains IDEs)
.vscode/               # Exclude Visual Studio Code settings